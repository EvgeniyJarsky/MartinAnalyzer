        </Grid.ColumnDefinitions>

        <Border Background="#FF8DB4E2" BorderBrush="Black" BorderThickness="6,6,3,3" Grid.Column="0" Grid.Row="0" Grid.RowSpan="2">
            <Label x:Name="mainTable_NN" VerticalAlignment="Center" Margin="2,0,2,0">
                <StackPanel>
                    <TextBlock Text="№№" FontWeight="Bold"/>
                    <TextBlock Text="колен" FontWeight="Bold"/>
                </StackPanel>
            </Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderBrush="Black" BorderThickness="3,6,3,1" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="3">
            <Label x:Name="mainTable_CountGrid" HorizontalAlignment="Center" FontWeight="Bold" >Количество сеток</Label>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="1">
            <Label x:Name="mainTable_CountGridSell" Margin="2,0,2,2" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="2">
            <Label x:Name="mainTable_CountGridBuy" Margin="2,0,2,2" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="3">
            <Label x:Name="mainTable_CountGridAll" Margin="2,0,2,2" FontWeight="Bold">All</Label>
        </Border>


        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="4" Grid.ColumnSpan="3">
            <StackPanel>
                <TextBlock x:Name="mainTable_SumLot1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Сумма лотов</TextBlock>
                <TextBlock x:Name="mainTable_SumLot2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">ордеров сеток</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="4">
            <Label x:Name="mainTable_SumLotSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="5">
            <Label x:Name="mainTable_SumLotBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="6">
            <Label x:Name="mainTable_SumLotAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="7" Grid.ColumnSpan="3">
            <StackPanel >
                <TextBlock x:Name="mainTable_TotalProfit1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Общая прибыль</TextBlock>
                <TextBlock x:Name="mainTable_TotalProfit2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">$</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="7">
            <Label x:Name="mainTable_TotalProfitSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="8">
            <Label x:Name="mainTable_TotalProfitBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="9">
            <Label x:Name="mainTable_TotalProfitAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="10" Grid.ColumnSpan="3">
            <StackPanel>
                <TextBlock x:Name="mainTable_AverageProfit1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Средняя прибыль</TextBlock>
                <TextBlock x:Name="mainTable_AverageProfit2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">сетки, $</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="10">
            <Label x:Name="mainTable_AverageProfitSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="11">
            <Label x:Name="mainTable_AverageProfitBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="12">
            <Label x:Name="mainTable_AverageProfitAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="13" Grid.ColumnSpan="3">
            <StackPanel >
                <TextBlock x:Name="mainTable_PercentProfit1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">% от общей</TextBlock>
                <TextBlock x:Name="mainTable_PercentProfit2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">прибыли</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="13">
            <Label x:Name="mainTable_PercentProfitSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="14">
            <Label x:Name="mainTable_PercentProfitBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="15">
            <Label x:Name="mainTable_PercentProfitAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="16" Grid.ColumnSpan="2">
            <StackPanel >
                <TextBlock x:Name="mainTable_MaxGridSize1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Макс. размер</TextBlock>
                <TextBlock x:Name="mainTable_MaxGridSize2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">сетки в пунктах</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="16">
            <Label x:Name="mainTable_MaxGridSizeSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="17">
            <Label x:Name="mainTable_MaxGridSizeBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="3,6,3,1" BorderBrush="Black" Grid.Row="0" Grid.Column="18" Grid.ColumnSpan="3">
            <StackPanel >
                <TextBlock x:Name="mainTable_AverageGridSize1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Средний размер</TextBlock>
                <TextBlock x:Name="mainTable_AverageGridSize2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">сетки в пунктах</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="3,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="18">
            <Label x:Name="mainTable_AverageGridSizeSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="0,0,1,3" BorderBrush="Black" Grid.Row="1" Grid.Column="19">
            <Label x:Name="mainTable_AverageGridSizeBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="0,0,3,3" BorderBrush="Black" Grid.Row="1" Grid.Column="20">
            <Label x:Name="mainTable_AverageGridSizeAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="0" Grid.Column="21" Grid.ColumnSpan="2">
            <StackPanel >
                <TextBlock x:Name="mainTable_MaxPointsToTP1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Макс. кол-во </TextBlock>
                <TextBlock x:Name="mainTable_MaxPointsToTP2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">пунктов до ТП</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="21">
            <Label x:Name="mainTable_MaxPointsToTPSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="22">
            <Label x:Name="mainTable_MaxPointsToTPBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="0" Grid.Column="23" Grid.ColumnSpan="3">
            <StackPanel >
                <TextBlock x:Name="mainTable_AveragePointsToTP1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Среднее кол-во</TextBlock>
                <TextBlock x:Name="mainTable_AveragePointsToTP2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">пунктов до ТП</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="23">
            <Label x:Name="mainTable_AveragePointsToTPSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="24">
            <Label x:Name="mainTable_AveragePointsToTPBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="25">
            <Label x:Name="mainTable_AveragePointsToTPAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="0" Grid.Column="26" Grid.ColumnSpan="2">
            <StackPanel >
                <TextBlock x:Name="mainTable_MaxLifeGrid1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Макс. время</TextBlock>
                <TextBlock x:Name="mainTable_MaxLifeGrid2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">жизни сетки</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="26">
            <Label x:Name="mainTable_MaxLifeGridSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="27">
            <Label x:Name="mainTable_MaxLifeGridBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>

        <Border Background="#FF8DB4E2" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="0" Grid.Column="28" Grid.ColumnSpan="3">
            <StackPanel >
                <TextBlock x:Name="mainTable_AverageLifeGrid1" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">Среднее время</TextBlock>
                <TextBlock x:Name="mainTable_AverageLifeGrid2" FontWeight="Bold" HorizontalAlignment="Center" Margin="2,0,2,0">жизни сетки</TextBlock>
            </StackPanel>
        </Border>
        <Border Background="#FFE6B8B7" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="28">
            <Label x:Name="mainTable_AverageLifeGridSell" Margin="2,0,2,0" FontWeight="Bold">Sell</Label>
        </Border>
        <Border Background="#FFD8E4BC" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="29">
            <Label x:Name="mainTable_AverageLifeGridBuy" Margin="2,0,2,0" FontWeight="Bold">Buy</Label>
        </Border>
        <Border Background="#FFC5D9F1" BorderThickness="1,1,0,0" BorderBrush="Black" Grid.Row="1" Grid.Column="30">
            <Label x:Name="mainTable_AverageLifeGridAll" Margin="2,0,2,0" FontWeight="Bold">All</Label>
        </Border>


        <Border BorderBrush="Black" BorderThickness="6,3,3,3" Grid.Column="0" Grid.Row="2">
            <ItemsControl x:Name="number_">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Background="#FF8DB4E2" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Border BorderThickness="0,0,0,1" BorderBrush="Black">
                            <Label HorizontalAlignment="Center" Content="{Binding countOrders}"></Label>
                        </Border>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

        <Border BorderBrush="Black" BorderThickness="3,3,0,3" Grid.Column="1" Grid.Row="2">
            <ItemsControl x:Name="GridCountSell">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Background="#FFE6B8B7" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                            <Label HorizontalAlignment="Center"  Content="{Binding countGridSell}"></Label>
                        </Border>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

        <Border BorderBrush="Black" BorderThickness="1,3,0,3" Grid.Column="2" Grid.Row="2">
            <ItemsControl x:Name="GridCountBuy">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Background="#FFD8E4BC" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
