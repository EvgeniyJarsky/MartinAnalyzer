                            <Label HorizontalAlignment="Center"  Content="{Binding countGridBuy}"></Label>
                        </Border>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

        <Border BorderBrush="Black" BorderThickness="1,3,3,3" Grid.Column="3" Grid.Row="2">
            <ItemsControl x:Name="GridCountAll">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Background="#FFC5D9F1" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                            <Label HorizontalAlignment="Center" Content="{Binding countGridAll}"></Label>
                        </Border>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

        <Border BorderBrush="Black" BorderThickness="3,3,0,3" Grid.Column="4" Grid.Row="2">
            <ItemsControl x:Name="SumLotSell">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
