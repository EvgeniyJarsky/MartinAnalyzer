                            <Label HorizontalAlignment="Center" Content="{Binding SumLotAll}"></Label>
                        </Border>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Border>

        <Border BorderBrush="Black" BorderThickness="3,3,0,3" Grid.Column="7" Grid.Row="2">
            <ItemsControl x:Name="TotalProfitSell">
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Background="#FFE6B8B7" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
